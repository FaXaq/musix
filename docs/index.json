    "content": "const config = require('config')\n\nclass Pitch {\n  static validateSci(pitch) {\n    if (!pitch) {\n      if (config.has('Pitch.sciDefault')) {\n        return config.get('Pitch.sciDefault')\n      }\n    } else if (typeof pitch === \"number\" &&\n               pitch > 0) {\n      return pitch\n    }\n\n    return new Error('Pitch is invalid', pitch)\n  }\n}\n\nmodule.exports = Pitch;\n",
